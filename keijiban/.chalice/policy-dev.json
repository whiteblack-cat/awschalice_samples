{
    "Version": "2012-10-17",
    "Statement": {
        "Effect": "Allow",
        "Action": [
            "logs:CreateLogGroup",
            "logs:CreateLogStream",
            "logs:PutLogEvents",
            "dynamodb:Query",
            "dynamodb:GetItem",
            "dynamodb:UpdateItem",
            "dynamodb:DeleteItem",
            "dynamodb:PutItem"
        ],
        "Resource": [
            "arn:aws:logs:*:*:*",
            "arn:aws:dynamodb:us-east-1:xxxxxxxxxx:table/keijiban"
        ]
    }
}
